$(function(){$(".fancybox").fancybox(),$(".con_text img, .con_desc img").not("a img:first-child").wrap(function(){return $('<a href="'+$(this).attr("src")+'" />').fancybox({transition:"none"})}),$("a.photobox").fancybox({rel:"gal",transition:"none",slideshow:!0,width:"650px",height:"650px"}),$(".bd_image_small").each(function(){var n=$(this).attr("src");$(this).wrap('<a class="orig" href="'+n.replace(/(small\/|medium\/)/,"medium/")+'" />'),$(".orig").fancybox({transition:"none"})}),$(".bb_img img").not("a img:first-child").each(function(){var n=$(this).attr("src");$(this).wrap('<a class="bb_photo" href="'+n+'" />'),$(".bb_photo").fancybox({transition:"none"})}),$(".forum_zoom a").each(function(){$(this).fancybox({transition:"none"})})});